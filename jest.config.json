{
    "collectCoverage": true,
    "coverageReporters": [
        "text"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 70,
            "functions": 95,
            "lines": 95,
            "statements": 95
        }
    },
    "preset": "ts-jest",
    "testRegex": "__tests__/(?!data|mocks).*\\.(ts|js|tsx|jsx)$",
    "rootDir": "src"
}
